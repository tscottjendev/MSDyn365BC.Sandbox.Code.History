        AEATTransFormatXML.Validate(Value, Value);
        AEATTransFormatXML.Validate(Box, Box);
        AEATTransFormatXML.Validate(Ask, Ask);
        AEATTransFormatXML.Validate("Exists Amount", ExistAmount);
        AEATTransFormatXML.Insert();
