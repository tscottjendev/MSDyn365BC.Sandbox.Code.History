    procedure InsertData("No.": Integer; Position: Integer; Length: Integer; Type: Option; Subtype: Option; Description: Text[250]; Value: Text[250]; Box: Code[5]; Amount: Boolean)
    var
        AEATTransFormat: Record "AEAT Transference Format";
    begin
        AEATTransFormat.Validate("VAT Statement Name", XStmt320);
        AEATTransFormat.Validate("No.", "No.");
        AEATTransFormat.Validate(Position, Position);
        AEATTransFormat.Validate(Length, Length);
        AEATTransFormat.Validate(Type, Type);
        AEATTransFormat.Validate(Subtype, Subtype);
        AEATTransFormat.Validate(Description, Description);
        AEATTransFormat.Validate(Value, Value);
        AEATTransFormat.Validate(Box, Box);
        AEATTransFormat.Validate("Exists Amount", Amount);
        AEATTransFormat.Insert();
    end;

    procedure InsertData2("No.": Integer; IndentationLevel: Integer; ParentLineno: Integer; LineType: Option; ValueType: Option; Description: Text[250]; Value: Text[250]; Box: Code[5]; Ask: Boolean; ExistAmount: Boolean)
